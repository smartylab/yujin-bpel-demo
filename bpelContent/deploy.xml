      <service name="kr.co.smartylab:DemoBPService" port="DemoBPPort"/>
    </provide>
    <invoke partnerLink="DynamicAdapter">
      <service name="adapter:concert_adapter_soap_serverService" port="concert_adapter_soap_server"/>
