</tns:DemoBPResponse>
</bpel:literal></bpel:from>
                <bpel:to variable="output" part="payload"></bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from part="parameters" variable="DynamicAdapterResponse">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[ns:out]]></bpel:query>
                </bpel:from>
                <bpel:to part="payload" variable="output">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:result]]></bpel:query>
                </bpel:to>
            </bpel:copy>
        </bpel:assign>
        <bpel:reply name="replyAllocationResult" partnerLink="client" portType="tns:DemoBP" operation="process" variable="output" />
        <bpel:pick name="CallTurtlebotService">
            <bpel:onMessage partnerLink="client" operation="invoke_first_service" portType="tns:DemoBP" variable="clientRequest">
                <bpel:sequence>
                    <bpel:assign validate="no" name="AssignTurtlebotServiceInput">
                        <bpel:copy>
                            <bpel:from><bpel:literal><tns:call_resource xmlns:tns="http://smartylab.co.kr/products/op/adapter" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tns:topic_name>tns:topic_name</tns:topic_name>
  <tns:message>
    <tns:linear>
      <tns:x>0.0</tns:x>
      <tns:y>0.0</tns:y>
      <tns:z>0.0</tns:z>
    </tns:linear>
    <tns:angular>
      <tns:x>0.0</tns:x>
      <tns:y>0.0</tns:y>
      <tns:z>0.0</tns:z>
    </tns:angular>
  </tns:message>
</tns:call_resource>
</bpel:literal></bpel:from>
                            <bpel:to variable="DynamicAdapterRequest1" part="parameters"></bpel:to>
                        </bpel:copy>
                        
                    </bpel:assign>
                    <bpel:invoke name="callTurtlebotMove" partnerLink="DynamicAdapter" operation="call_resource" portType="ns:concert_adapter_soap_serverPortType" inputVariable="DynamicAdapterRequest1" outputVariable="DynamicAdapterResponse1"></bpel:invoke>
                    <bpel:assign validate="no" name="AssignTurtlebotMoveResult">
                        <bpel:copy>
                            <bpel:from><bpel:literal><tns:invoke_first_serviceResponse xmlns:tns="kr.co.smartylab" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tns:out>tns:out</tns:out>
</tns:invoke_first_serviceResponse>
</bpel:literal></bpel:from>
                            <bpel:to variable="clientResponse" part="parameters"></bpel:to>
                        </bpel:copy>
                        
                    </bpel:assign>
                    
                    
                    <bpel:reply name="ReplyFirstServiceExecutionResult" partnerLink="client" operation="invoke_first_service" portType="tns:DemoBP" variable="clientResponse"></bpel:reply>
                </bpel:sequence>
            </bpel:onMessage>
            
        </bpel:pick>
        
        
        
        
        
        
        
    </bpel:sequence>
