        <tns:uri>tns:uri</tns:uri>
        <tns:max>0</tns:max>
        <tns:min>0</tns:min>
        <tns:parameters>
          <tns:parameter>
            <tns:message>tns:message</tns:message>
            <tns:frequency>0</tns:frequency>
          </tns:parameter>
        </tns:parameters>
      </tns:Node>
    </tns:nodes>
    <tns:topics>
