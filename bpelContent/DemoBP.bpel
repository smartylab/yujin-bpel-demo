      </tns:Edge>
    </tns:edges>
    <tns:methods>
      <tns:Method>
        <tns:name>invoke_first_service</tns:name>
        <tns:address>http://172.16.113.1:8080/ode/processes/DemoBP</tns:address>
        <tns:namespace>kr.co.smartylab</tns:namespace>
        <tns:return_name>[invokeFirstAppResponse,out]</tns:return_name>
      </tns:Method>
    </tns:methods>
  </tns:LinkGraph>
</tns:invoke_adapter></bpel:literal></bpel:from>
                <bpel:to variable="DynamicAdapterRequest" part="parameters"></bpel:to>
            </bpel:copy>
            
        </bpel:assign>
        <bpel:invoke name="RequestToAllocateResources" partnerLink="DynamicAdapter" operation="invoke_adapter" portType="ns:concert_adapter_soap_serverPortType" inputVariable="DynamicAdapterRequest" outputVariable="DynamicAdapterResponse"></bpel:invoke>
        <bpel:assign validate="no" name="AssignAllocationResult">
            <bpel:copy>
                <bpel:from><bpel:literal><tns:DemoBPResponse xmlns:tns="kr.co.smartylab" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
